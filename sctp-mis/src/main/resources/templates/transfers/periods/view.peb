    {% extends "../base" %}
    {% import "../../utils/form" %}

    {% block content %}
     <div class="container">
        <div class="card no-overlap">
            <header class="card-header">
                <p class="card-header-title">Transfer Period Details</p>
            </header>
            <div class="card-content">
                <div class="content">
                   <View-Period :transfer-period-id="{{ transferPeriod.id }}" />
