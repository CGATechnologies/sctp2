            </div>
        </div>
    </div>
    {% endblock %}

    {% block footerScripts %}
    {{ parent() }}
    <script type="module">
        const App = {
            el: '#vapp',

            mounted() {
                console.log('application mounted. ')
            },

            created() {
                console.log('application created')
            },

            components: {
                'ViewPeriod': httpVueLoader('/components/transfers/periods/ViewPeriod.vue')
            }
        };

        window.addEventListener('load', () => {
            var app = new Vue(App);
        });
    </script>
    {% endblock %}